@model GoTicket.Models.Ticket
@{
    ViewBag.Title = "NewCustomer";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Yeni Ticket Ekleme</title>
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
</head>
<body>
    <form class="form-group" method="post">
        @Html.LabelFor(x => x.vehicle_type, "Vehicle Type")
        @Html.TextBoxFor(x => x.vehicle_type, new { @class = "form-control" })
        <br />
        @Html.LabelFor(x => x.deperture_date, "Deperture Date")
        @Html.TextBoxFor(x => x.deperture_date, new { @class = "form-control", type = "datetime-local" })
        <br />
        @Html.LabelFor(x => x.arrival_date, "Arrival Date")
        @Html.TextBoxFor(x => x.arrival_date, new { @class = "form-control", type = "datetime-local" })
        <br />
        <button class="btn btn-primary">Add Ticket</button>
    </form>
</body>
</html>